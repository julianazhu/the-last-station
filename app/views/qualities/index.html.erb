<h1>Existing Qualities</h1>
 
<table>
  <tr>
    <th>Name</th>
  </tr>
 
  <% @qualities.each do |quality| %>
    <tr>
      <td><%= link_to quality.name, edit_quality_path(quality) %></td>
      <td><%= link_to quality.description, edit_quality_path(quality) %></td>
      <td><%= link_to 'Destroy', quality_path(quality), method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>